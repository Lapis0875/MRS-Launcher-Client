<Window x:Class="MRSLauncherClient.JavaDownloadWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MRSLauncherClient"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        WindowStartupLocation="CenterScreen"
        Title="" Height="130" Width="300"
        Loaded="Window_Loaded"
        WindowStyle="None"
        >

    <Grid>
        <ProgressBar Name="pbProgress" Height="24" Width="252" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,0,0,20"/>
        <Label Name="lvStatus" Content="자바 다운로드 중" FontSize="15" Width="252" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Height="28" VerticalAlignment="Top" Margin="0,15,0,0"/>
        <Label Name="lvWarn" Content="(강제로 창을 끄지 마세요)" FontSize="13" Width="252" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Height="28" Margin="0,0,0,10"/>

    </Grid>
</Window>
