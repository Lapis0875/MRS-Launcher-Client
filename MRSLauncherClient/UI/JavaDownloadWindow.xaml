<Window x:Class="MRSLauncherClient.JavaDownloadWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MRSLauncherClient"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        WindowStartupLocation="CenterScreen"
        Title="" Height="130" Width="300"
        Loaded="Window_Loaded"
        >

    <Canvas Margin="0,-27,0,0">
        <ProgressBar Name="pbProgress" Height="23" Canvas.Left="24" Canvas.Top="77" Width="252"/>
        <Label Name="lvStatus" Content="자바 다운로드 중" FontSize="15" Canvas.Left="24" Canvas.Top="21" Width="252" HorizontalContentAlignment="Center" VerticalContentAlignment="Top" Height="28"/>
        <Label Name="lvWarn" Content="(강제로 창을 끄지 마세요)" FontSize="13" Canvas.Left="24" Canvas.Top="44" Width="252" HorizontalContentAlignment="Center" VerticalContentAlignment="Top" Height="28"/>

    </Canvas>
</Window>
