<Window x:Class="MRSLauncherClient.JavaDownloadWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MRSLauncherClient"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        WindowStartupLocation="CenterScreen"
        Title="JavaDownloadWindow" Height="140" Width="300"
        Loaded="Window_Loaded">
    
    <Canvas>
        <ProgressBar Name="pbProgress" Height="23" Canvas.Left="21" Canvas.Top="55" Width="252"/>
        <Label Name="lvStatus" Content="자바 다운로드 중" Canvas.Left="21" Canvas.Top="24" Width="252" HorizontalContentAlignment="Center"/>

    </Canvas>
</Window>
